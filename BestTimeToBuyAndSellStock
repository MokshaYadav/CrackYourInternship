class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int n=prices.size();
        int kharida=prices[0];
        int profit=0;
        for(int i=0;i<n;i++){
            if(prices[i] < kharida){
                kharida= prices[i];
            }
            else if( prices[i]-kharida > profit){
                profit=prices[i] - kharida;
            }
        }
        return profit;
        
    }
};
